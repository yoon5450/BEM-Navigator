// .vscode/launch.json
{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run Extension",
      "type": "extensionHost",
      "request": "launch",
      "runtimeExecutable": "${execPath}",
      "args": ["--extensionDevelopmentPath=${workspaceFolder}"],
      "outFiles": [
        "${workspaceFolder}/dist/**/*.js" // 여기를 out에서 dist로 변경
      ]
      // "preLaunchTask": "npm: watch:esbuild"
    }
  ]
}
